        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PumaKatari.Screens"
        mc:Ignorable="d"
        Title="Registro_Empleados" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgEmpelados" Margin="70,74,310,74"/>
        
        
        <Button x:Name="btnCrearRegEmp" Content="Crear" HorizontalAlignment="Left" Margin="580,74,0,0" VerticalAlignment="Top" Height="50" Width="135" Click="btnCrearRegEmp_Click"/>
