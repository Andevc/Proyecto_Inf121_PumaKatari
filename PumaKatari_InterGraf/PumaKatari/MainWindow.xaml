<Window x:Class="PumaKatari.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PumaKatari"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnArchFR" Content="Fechas Recorrido" HorizontalAlignment="Center" Margin="0,34,0,0" VerticalAlignment="Top" Width="240" Height="45" Click="btnArchRF_click"/>
        <Button x:Name="btnArchBus" Content="Registro buses" HorizontalAlignment="Center" Margin="0,109,0,0" VerticalAlignment="Top" Width="240" Height="42" Click="btnArchBus_click"/>
        <Button x:Name="btnEmpleados" Content="EMpleados" HorizontalAlignment="Center" Margin="0,174,0,0" VerticalAlignment="Top" Width="240" Height="47" Click="nbtnmpleados_Click"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,244,0,0" VerticalAlignment="Top" Width="240" Height="39"/>
        <Button x:Name="exit" Content="Salir" HorizontalAlignment="Center" Margin="0,309,0,0" VerticalAlignment="Top" Width="240" Height="50" Click="exit_click"/>

    </Grid>
</Window>
